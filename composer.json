{
    "name": "prezent/crud-bundle",
    "type": "library",
    "description": "Easy CRUD for Symfony",
    "keywords": ["crud", "admin", "grid", "datagrid"],
    "homepage": "http://www.prezent.nl",
    "require": {
        "php": "^5.5|^7.0",
        "prezent/grid-bundle": "^0.4.0",
        "sensio/framework-extra-bundle": "^3.0|^4.0|^5.0",
        "symfony/framework-bundle": "^2.6|^3.0|^4.0",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.5.2",
        "doctrine/orm": "^2.5",
        "symfony/css-selector": "^2.6|^3.0|^4.0",
        "symfony/browser-kit": "^2.7|^3.0|^4.0",
        "symfony/finder": "^2.6|^3.0|^4.0",
        "symfony/form": "^2.6|^3.0|^4.0",
        "symfony/phpunit-bridge": "^3.3|^4.0",
        "symfony/templating": "^2.6|^3.0|^4.0",
        "symfony/yaml": "^2.6|^3.0|^4.0",
        "doctrine/doctrine-fixtures-bundle": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander Marechal",
            "email": "sander@prezent.nl"
        },
        {
            "name": "Prezent Internet B.V.",
            "email": "github@prezent.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Prezent\\CrudBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Prezent\\CrudBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
