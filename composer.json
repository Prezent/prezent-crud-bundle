{
    "name": "prezent/crud-bundle",
    "type": "symfony-bundle",
    "description": "Easy CRUD for Symfony",
    "keywords": ["crud", "admin", "grid", "datagrid"],
    "homepage": "http://www.prezent.nl",
    "require": {
        "php": "^7.0",
        "prezent/grid-bundle": "^0.5.0",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "babdev/pagerfanta-bundle": "^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.5.2|^2.0",
        "doctrine/doctrine-fixtures-bundle": "^2.2|^3.0",
        "doctrine/orm": "^2.5",
        "symfony/css-selector": "^4.0|^5.0",
        "symfony/browser-kit": "^4.0|^5.0",
        "symfony/finder": "^4.0|^5.0",
        "symfony/form": "^4.0|^5.0",
        "symfony/phpunit-bridge": "^4.2.7|^5.0",
        "symfony/templating": "^4.0|^5.0",
        "symfony/twig-bundle": "^4.0|^5.0",
        "symfony/validator": "^4.0|^5.0",
        "symfony/yaml": "^4.0|^5.0",
        "symfony/monolog-bundle": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sander Marechal",
            "email": "sander@prezent.nl"
        },
        {
            "name": "Prezent Internet B.V.",
            "email": "github@prezent.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Prezent\\CrudBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Prezent\\CrudBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
