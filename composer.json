{
    "name": "prezent/crud-bundle",
    "type": "symfony-bundle",
    "description": "Easy CRUD for Symfony",
    "keywords": ["crud", "admin", "grid", "datagrid"],
    "homepage": "https://www.prezent.nl",
    "require": {
        "php": "^8.2",
        "babdev/pagerfanta-bundle": "^4.0",
        "pagerfanta/core": "^3.1",
        "pagerfanta/doctrine-orm-adapter": "^3.1",
        "pagerfanta/twig": "^4.0",
        "prezent/grid-bundle": "dev-symfony7-upgrade@dev",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/validator": "^7.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/orm": "^2.5",
        "symfony/css-selector": "^7.0",
        "symfony/browser-kit": "^7.0",
        "symfony/finder": "^7.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/monolog-bundle": "^3.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Prezent Internet B.V.",
            "email": "github@prezent.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Prezent\\CrudBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Prezent\\CrudBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
